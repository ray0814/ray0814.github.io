<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="lib_ui_view_CTickView">
        <attr name="lib_ui_view_ctv_color" format="color" />
        <attr name="lib_ui_view_ctv_colorCircle" format="color" />
        <attr name="lib_ui_view_ctv_strokeWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="lib_ui_view_LrcView">
        <attr name="lib_ui_view_lrc_textColor" format="color" />
        <attr name="lib_ui_view_lrc_textColorCur" format="color" />
        <attr name="lib_ui_view_lrc_timeColor" format="color" />
        <attr name="lib_ui_view_lrc_textSize" format="dimension" />
        <attr name="lib_ui_view_lrc_textSizeCur" format="dimension" />
        <attr name="lib_ui_view_lrc_timeSize" format="dimension" />
