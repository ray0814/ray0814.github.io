        <attr name="lib_ui_view_advertswitcher_timeSpan" format="integer" />
        <attr name="lib_ui_view_advertswitcher_inAnim" format="reference" />
        <attr name="lib_ui_view_advertswitcher_outAnim" format="reference" />
        <attr name="lib_ui_view_advertswitcher_interpolator" format="reference" />
